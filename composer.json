{
  "name": "osim/craft-focus",
  "description": "A Craft CMS plugin that integrates the focus.osim.digital accessibility checker.",
  "type": "craft-plugin",
  "keywords": [
      "craft",
      "cms",
      "craftcms",
      "craft-plugin",
      "a11y",
      "accessibility",
      "focus"
  ],
  "license": "proprietary",
  "authors": [
    {
      "name": "OSiM Interactive",
      "homepage": "https://osim.digital"
    }
  ],
  "support": {
    "email": "support@osim.digital",
    "issues": "https://github.com/osim-interactive/craft-osim-focus/issues?state=open",
    "source": "https://github.com/osim-interactive/craft-osim-focus",
    "docs": "https://github.com/osim-interactive/craft-osim-focus/blob/main/README.md"
  },
  "require": {
    "craftcms/cms": "^5.0.0",
    "xlient/xml-sitemap": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "osim\\craft\\focus\\": "src/"
    }
  },
  "extra": {
    "name": "OSiM Focus",
    "handle": "osim-focus"
  },
  "require-dev": {
    "phpstan/phpstan": "v1.*",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
